<div id="content-header">
  <%#= image_tag("examination/show_exam.png") %>
  <h1>
    <%= t('exams_text') %>
  </h1>
  <h3>
    <%= t('view_transcripts') %>
  </h3>
  <div id="app-back-button">
    <%#= back_button %>
  </div>
</div>
<div id="page-yield">
  <% unless flash[:notice].nil? %>
    <p class="flash-msg">
      <%= flash[:notice] %>
    </p>
  <% end %>
  <div class="bread_crumb">
    <%= link_to t('exam_text'), exam_index_path %> »
    <%= link_to t('report_center'), report_center_exam_index_path %>  »
    <%= t('view_transcripts') %>
  </div>
  <div class="box">
    <%= form_for :transcript, url: student_transcript_exam_index_path do |x| %>
      <div class="label-field-pair">
        <label for="student_course">
          <%= t('select_a_batch') %>
          :
        </label>
        <div class="text-input-bg">
          <%= select :transcript, :batch_id, @batches.map{|b| [b.full_name, b.id]},{prompt: t('select_a_batch')} %>
        </div><br>
      </div>
      <%= submit_tag "", value: "► #{t('view')}", class: :'btn btn-sm btn-pink' %>
    <% end %>
    <div class="extender"></div>
  </div>
</div>
