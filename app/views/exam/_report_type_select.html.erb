<div class="label-field-pair">
  <label for="student_course">
    <%= t('select_ranking_level') %>
    :
  </label>
  <div class="text-input-bg">
    <%= select :ranking_level_report, :ranking_level_id, @ranking_levels.map {|b| [b.name, b.id]},{:prompt => "select a ranking level"}   %>
  </div>
</div>
<div class="label-field-pair">
  <label for="student_course">
    <%= t('select_a_report_type') %>
    :
  </label>
  <div class="text-input-bg">
    <%= select :ranking_level_report, :report_type, [["#{t('overall')}","overall"],["#{t('subject_wise')}","subject"]], {:prompt => "#{t('select_a_report_type')}"}, {:onChange => "onChangeRequest(this, '#{url_for(action: :select_type, batch_id: @batch.id, report_type: 'paramid')}')"} %>
  </div>
</div>
<div id="subject-select"></div>
